{
    "name": "exolnet/laravel-segment",
    "description": "Laravel module to launch features at people.",
    "keywords": ["laravel", "feature", "aim", "ab", "target"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre D'Eschambeault",
            "email": "adeschambeault@exolnet.com"
        }
    ],
    "support": {
        "issues": "https://github.com/eXolnet/laravel-segment/issues",
        "source": "https://github.com/eXolnet/laravel-segment"
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/view": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Exolnet\\Segment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exolnet\\Segment\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Exolnet\\Segment\\SegmentServiceProvider"
            ]
        }
    }
}
